// define object
def u = "User"
def a = "Alice"
def b = "Bob"

// send message
msg u -> a = "Hi!"
msg a -> b = "User says hi!"
msg b -> b = "Who is User!"

// send response
rsp b -> a = "Bob is done"
rsp b -> a = "Alice is done"

// raw string
rsp b -> a  = `line1
line2`

alt "text"
  msg u -> a = "hi"
else
  msg u -> a = "bye"
end

// unformatted code
alt "foo"
loop "text"
msg u->a = "hi"
end
end
